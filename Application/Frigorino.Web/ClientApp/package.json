{
    "name": "frigorino.client",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "start": "vite --host",
        "build": "tsc -b && vite build",
        "preview": "vite preview",
        "api": "npx openapi-typescript-codegen -i ./src/lib/swagger.json -o ./src/lib/api --client fetch --useUnionTypes --name FrigorinoApiClient",
        "lint": "eslint .",
        "prettier": "prettier --write .",
        "fix": "eslint . --fix && prettier --write .",
        "sql": "docker run -p 8080:80 -e \"PGADMIN_DEFAULT_EMAIL=test@test.de\" -e \"PGADMIN_DEFAULT_PASSWORD=test\" -v pgadmin-data:/var/lib/pgadmin --name pgadmin -d dpage/pgadmin4"
    },
    "dependencies": {
        "@dnd-kit/core": "^6.3.1",
        "@dnd-kit/sortable": "^10.0.0",
        "@dnd-kit/utilities": "^3.2.2",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mui/icons-material": "^7.2.0",
        "@mui/material": "^7.2.0",
        "@tanstack/react-query": "^5.83.0",
        "@tanstack/react-router": "^1.128.8",
        "@tanstack/react-router-devtools": "^1.128.8",
        "firebase": "^12.0.0",
        "i18next": "^25.4.2",
        "i18next-browser-languagedetector": "^8.2.0",
        "i18next-http-backend": "^3.0.2",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-i18next": "^15.7.2",
        "react-window": "^1.8.11",
        "sonner": "^2.0.7",
        "zustand": "^5.0.6"
